{
    "name": "demo",
    "version": "3.0.0",
    "private": true,
    "homepage": "https://marmelab.com/ra-enterprise-demo",
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "start": "export NPM_TOKEN=$NPM_TOKEN && react-scripts start",
        "build": "export NPM_TOKEN=$NPM_TOKEN && react-scripts build",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build",
        "lint": "eslint --ext .js,.ts,.tsx \"./src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint --ext .js,.ts,.tsx \"./src/**/*.{js,ts,tsx}\" --fix",
        "prettier": "prettier --config ./.prettierrc.js --write --list-different \"./src/**/*.{js,json,ts,tsx,css,md}\"",
        "e2e-dev": "cypress open",
        "e2e-run": "cypress run --browser chrome --headless",
        "e2e-serve": "cd build && serve -c ../cypress/serve.json -l 3000",
        "e2e": "start-server-and-test e2e-serve http://localhost:3000 e2e-run"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 10",
        "not op_mini all"
    ],
    "dependencies": {
        "@material-ui/core": "^4.11.3",
