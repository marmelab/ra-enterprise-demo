    "scripts": {
        "preview": "vite preview",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "start": "vite --force",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 tsc && NODE_OPTIONS=--max_old_space_size=4096 vite build",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build",
        "lint": "eslint --ext .js,.ts,.tsx \"./src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint --ext .js,.ts,.tsx \"./src/**/*.{js,ts,tsx}\" --fix",
        "prettier": "prettier --config ./.prettierrc.js --write --list-different \"./src/**/*.{js,json,ts,tsx,css,md}\"",
        "e2e-dev": "cypress open",
        "e2e-run": "cypress run --browser chrome --headless",
        "e2e-serve": "serve -c ../cypress/serve.json -l 3000 build",
        "e2e": "start-server-and-test e2e-serve http://localhost:3000 e2e-run",
        "type-check": "tsc"
    },
    "homepage": ".",
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 10",
        "not op_mini all"
    ],
    "devDependencies": {
        "@testing-library/cypress": "^7.0.6",
