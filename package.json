        "@react-admin/ra-search": "^2.2.1",
        "@react-admin/ra-tour": "^1.1.2",
        "@react-admin/ra-tree": "^1.6.5",
        "data-generator-retail": "^3.15.1",
        "date-fns": "^1.29.0",
        "fakerest": "^3.3.0",
        "fetch-mock": "^9.4.0",
